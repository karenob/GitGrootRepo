@using IAmGroot2.Extensions;
@{
    ViewBag.Title = "Home Page";
    string taskToken = ViewBag.TaskToken;
    Layout = "~/Views/Shared/_ApplicationLayout.cshtml";
}
<terminateComponent taskToken="@taskToken"></terminateComponent>
@section Scripts{
    @if (Html.IsRelease())
    {
        <script src="~/Scripts/bundles/TerminateComponent.min.js"></script>
    }
    else
    {
        <script type="text/javascript">System.import('Scripts/Angular/TerminateComponent');</script>
    }
}

